{
  "name": "@simonsmith/cypress-image-snapshot",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "test:open": "npm-run-all build --parallel cypress:server cypress:open",
    "build": "run-s build:clean build:ts",
    "build:clean": "rm -rf dist",
    "build:ts": "tsc",
    "lint": "run-p -c 'lint:*'",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint --ext .ts,.json .",
    "cypress:open": "cypress open --browser electron --e2e",
    "cypress:server": "http-server -p 9001 -c-1 ./cypress/server"
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.9.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "cypress": ">10.0.0"
  },
  "dependencies": {
    "jest-image-snapshot": "^6.1.0",
    "just-extend": "^6.2.0"
  }
}
